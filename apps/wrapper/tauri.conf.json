{
  "$schema": "../../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://localhost:4200",
    "distDir": "..\\..\\dist\\apps\\frontend"
  },
  "package": {
    "productName": "DevConsole",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "http": {
        "all": true,
        "scope": [
          "https://**",
          "http://**"
        ]
      },
      "window": {
        "create": true,
        "close": true,
        "hide": true
      },
      "process": {
        "exit": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "open": true,
        "confirm": true,
        "save": true
      },
      "fs": {
        "all": true,
        "scope": [
          "**/*"
        ]
      },
      "shell": {
        "execute": true,
        "sidecar": true,
        "scope": [
          {
            "name": "cmd",
            "cmd": "cmd",
            "args": true
          },
          {
            "sidecar": true,
            "name": "sidecars/port-qry",
            "args": true
          }
        ]
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [
        "sidecars/port-qry"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "de.keyle.dev-console",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "shortDescription": "",
      "targets": [
        "msi",
        "updater"
      ],
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "allowDowngrades": true,
        "webviewInstallMode": {
          "type": "embedBootstrapper",
          "silent": false
        },
        "timestampUrl": "",
        "wix": {
          "language": "en-US"
        }
      }
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://update-dev-console.keyle.de/update/check"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU1ODFCNkQwQTdFQTFBQUMKUldTc0d1cW4wTGFCNWZtSkFmTXFZNDNXQUNIL1hVTlVQRTJNR1p6ZGpyVkhPRGFLbFBtNE9MU2cK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "DevConsole",
        "width": 800,
        "height": 600,
        "minWidth": 860,
        "minHeight": 350
      }
    ]
  }
}
