{
  "cli": {
    "packageManager": "pnpm"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "scss",
      "type": "component"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true,
      "prefix": "cl",
      "type": "component"
    },
    "@schematics/angular:class": {
      "skipTests": true
    },
    "@schematics/angular:directive": {
      "skipTests": true,
      "prefix": "",
      "type": "directive"
    },
    "@schematics/angular:guard": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:service": {
      "skipTests": true,
      "type": "service"
    },
    "@nx/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "directive": {
        "type": "directive"
      },
      "service": {
        "type": "service"
      },
      "scam": {
        "type": "component"
      },
      "scam-directive": {
        "type": "directive"
      },
      "guard": {
        "typeSeparator": "."
      },
      "interceptor": {
        "typeSeparator": "."
      },
      "module": {
        "typeSeparator": "."
      },
      "pipe": {
        "typeSeparator": "."
      },
      "resolver": {
        "typeSeparator": "."
      }
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  },
  "defaultProject": "frontend",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/angular.json",
      "{workspaceRoot}/tsconfig.base.json",
      "{workspaceRoot}/tslint.json",
      "{workspaceRoot}/nx.json"
    ],
    "production": [
      "default",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/eslint.config.mjs"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "production",
        "^production"
      ],
      "cache": true
    },
    "test": {
      "inputs": [
        "default",
        "^production",
        "{workspaceRoot}/jest.preset.js"
      ],
      "cache": true
    },
    "@nx/eslint:lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/eslint.config.mjs"
      ],
      "cache": true
    }
  },
  "parallel": 1,
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ]
}
