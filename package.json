{
  "name": "dev-console",
  "author": "scomans",
  "productName": "DevConsole",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && exitzero electron-builder install-app-deps",
    "nx": "nx",
    "start": "ng serve",
    "start:all": "run-p start:frontend start:wrapper",
    "start:frontend": "nx serve frontend",
    "start:wrapper": "wait-on http://localhost:4200 && nx serve wrapper",
    "build": "ng build",
    "build:all": "nx build frontend && nx build wrapper",
    "make": "nx build frontend --prod && nx build wrapper --prod && nx run wrapper:make",
    "make:wrapper": "nx build wrapper --prod && nx run wrapper:make",
    "update-version": "node tools/ci/update-version.js",
    "release": "node tools/ci/release.js"
  },
  "private": true,
  "pnpm": {
    "auto-install-peers": true,
    "overrides": {
      "ini@<1.3.6": ">=1.3.6",
      "nx@<14": ">=14"
    },
    "packageExtensions": {
      "nx-electron": {
        "@nrwl/devkit": "14.x",
        "@nx/workspace": "14.x",
        "nx": "14.x"
      }
    }
  },
  "dependencies": {
    "@angular/animations": "16.1.5",
    "@angular/cdk": "16.1.5",
    "@angular/common": "16.1.5",
    "@angular/compiler": "16.1.5",
